!!!
html
  head
    title= "SWAG"
    link(rel='stylesheet', href='/stylesheets/bootstrap.css')
  body
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          .brand
            a(href='/') SWAG
          .nav-collapse
            ul.nav
              li
                a(href='/') these 
              li
                a(href='/') do 
              li
                a(href='/') nothing 
    .wrap
      .container
        .content
          != body
    .sidebar
      ul.tags
        - each tag in allTags
          li
            a(href=tagUrl(tag)) #{tag}
