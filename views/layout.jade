!!!
html
  head
    title= "SWAG"
    link(rel='stylesheet', href='/css/normalize.css')
    link(rel='stylesheet', href='/css/layout.css')
  body
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          .brand
            a(href='/') SWAG
          .nav-collapse
            ul.nav
              li
                a(href='/') these 
              li
                a(href='/') do 
              li
                a(href='/') nothing 
    .wrap
      .container
        .content
          != body
      .sidebar
        h2 categories
        ul.categories
          - each cat in allCategories
            li
              a(href=categoryUrl(cat)) #{cat}
        h2 tags
        ul.tags
          - each tag in allTags
            li
              a(href=tagUrl(tag)) #{tag}
